 from \"../store\";\r\nimport axios from \"axios\";\r\n// import router from \"../router\";\r\n\r\nexport default {\r\n  components: {\r\n    homeLayout,\r\n  },\r\n  data() {\r\n    return {\r\n      emp_account: \"\",\r\n      emp_password: \"\",\r\n      items: null,\r\n      empList: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    var jsonData = {\r\n       // eslint-disable-next-line no-undef\r\n   emp_no: $(\"#emp_no\").val(),\r\n           // eslint-disable-next-line no-undef\r\n          emp_account: $(\"#emp_account\").val(),\r\n           // eslint-disable-next-line no-undef\r\n          emp_password: $(\"#emp_password\").val(),\r\n           // eslint-disable-next-line no-undef\r\n          ch_name: $(\"#ch_name\").val(),\r\n};\r\n    // eslint-disable-next-line no-undef\r\n    $(\"#addItem\").click(function () {\r\n      // eslint-disable-next-line no-undef\r\n      $.ajax({\r\n        url: \"/user/add\",\r\n        method: \"POST\",\r\n        data: JSON.stringify(jsonData),\r\n        // eslint-disable-next-line no-unused-vars\r\n        success: function (response) {\r\n          alert(\"新增成功\");\r\n        },\r\n        error: function (error) {\r\n          console.error(error);\r\n        },\r\n      });\r\n    });\r\n    // eslint-disable-next-line no-undef\r\n    $(\"#deleteItem\").click(function () {\r\n      // eslint-disable-next-line no-undef\r\n      $.ajax({\r\n        url: \"/user/add\",\r\n        method: \"POST\",\r\n        // eslint-disable-next-line no-unused-vars\r\n        success: function (response) {\r\n          console.log(response.status);\r\n        },\r\n        error: function (error) {\r\n          console.error(error);\r\n        },\r\n      });\r\n    });\r\n  },\r\n  methods: {\r\n    queryItem() {\r\n      axios\r\n        .post(\"/user/query\", {\r\n          emp_account: this.emp_account,\r\n          emp_password: this.emp_password,\r\n        })\r\n        .then((response) => {\r\n          this.items = response.data;\r\n          console.log(this.items.ch_name);\r\n          this.empList = [];\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    queryAllItem() {\r\n      axios\r\n        .post(\"/user/queryAll\")\r\n        .then((response) => {\r\n          this.empList = response.data;\r\n          this.items = null;\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.modal {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.modal-content {\r\n  background-color: #fff;\r\n  padding: 20px;\r\n  text-align: center;\r\n}\r\n\r\nbutton {\r\n  margin-top: 10px;\r\n}\r\n</style>\r\n"],"mappings":"AAoCA,OAAOA,UAAS,MAAO,2BAA2B;AAClD;AACA,OAAOC,KAAI,MAAO,OAAO;AACzB;;AAEA,eAAe;EACbC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,YAAY,EAAE,EAAE;MAChBC,KAAK,EAAE,IAAI;MACXC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAIC,QAAO,GAAI;MACZ;MACJC,MAAM,EAAEC,CAAC,CAAC,SAAS,CAAC,CAACC,GAAG,CAAC,CAAC;MAClB;MACDR,WAAW,EAAEO,CAAC,CAAC,cAAc,CAAC,CAACC,GAAG,CAAC,CAAC;MACnC;MACDP,YAAY,EAAEM,CAAC,CAAC,eAAe,CAAC,CAACC,GAAG,CAAC,CAAC;MACrC;MACDC,OAAO,EAAEF,CAAC,CAAC,UAAU,CAAC,CAACC,GAAG,CAAC;IACrC,CAAC;IACG;IACAD,CAAC,CAAC,UAAU,CAAC,CAACG,KAAK,CAAC,YAAY;MAC9B;MACAH,CAAC,CAACI,IAAI,CAAC;QACLC,GAAG,EAAE,WAAW;QAChBC,MAAM,EAAE,MAAM;QACdd,IAAI,EAAEe,IAAI,CAACC,SAAS,CAACV,QAAQ,CAAC;QAC9B;QACAW,OAAO,EAAE,SAAAA,CAAUC,QAAQ,EAAE;UAC3BC,KAAK,CAAC,MAAM,CAAC;QACf,CAAC;QACDC,KAAK,EAAE,SAAAA,CAAUA,KAAK,EAAE;UACtBC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;QACtB;MACF,CAAC,CAAC;IACJ,CAAC,CAAC;IACF;IACAZ,CAAC,CAAC,aAAa,CAAC,CAACG,KAAK,CAAC,YAAY;MACjC;MACAH,CAAC,CAACI,IAAI,CAAC;QACLC,GAAG,EAAE,WAAW;QAChBC,MAAM,EAAE,MAAM;QACd;QACAG,OAAO,EAAE,SAAAA,CAAUC,QAAQ,EA