\n    };\r\n  },\r\n   methods: {\r\n    queryItem() {\r\n     axios\r\n        .post(\"/user/query\", {\r\n          emp_no: this.emp_no,\r\n          ch_name: this.ch_name,\r\n        })\r\n        .then((response) => {\r\n          this.sessionData = response.data;\r\n\r\n          //登入成功後跳轉頁面\r\n          if (this.sessionData != \"\") {\r\n            router.push(\"/home\");\r\n            store.commit(\"setEmpId\", \"456\");\r\n          } else {\r\n            alert(\"登入失敗\");\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.modal {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.modal-content {\r\n  background-color: #fff;\r\n  padding: 20px;\r\n  text-align: center;\r\n}\r\n\r\nbutton {\r\n  margin-top: 10px;\r\n}\r\n</style>\r\n"],"mappings":";AAeA,OAAOA,UAAS,MAAO,2BAA2B;AAClD,OAAOC,KAAI,MAAO,UAAU;AAC5B,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,WAAW;AAG9B,eAAe;EACbC,UAAU,EAAE;IACVJ;EACF,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,OAAO;MACJA,IAAI,EAAE;QACLC,MAAM,EAAE,EAAE;QACVC,OAAO,EAAE,EAAE;QACXC,KAAK,EAAE;MACT,CAAC;MACDC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACAC,OAAO,EAAE;IACRC,SAASA,CAAA,EAAG;MACXT,KAAI,CACAU,IAAI,CAAC,aAAa,EAAE;QACnBN,MAAM,EAAE,IAAI,CAACA,MAAM;QACnBC,OAAO,EAAE,IAAI,CAACA;MAChB,CAAC,EACAM,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACC,WAAU,GAAID,QAAQ,CAACT,IAAI;;QAEhC;QACA,IAAI,IAAI,CAACU,WAAU,IAAK,EAAE,EAAE;UAC1BZ,MAAM,CAACa,IAAI,CAAC,OAAO,CAAC;UACpBf,KAAK,CAACgB,MAAM,CAAC,UAAU,EAAE,KAAK,CAAC;QACjC,OAAO;UACLC,KAAK,CAAC,MAAM,CAAC;QACf;MACF,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;QAChBC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACN;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}