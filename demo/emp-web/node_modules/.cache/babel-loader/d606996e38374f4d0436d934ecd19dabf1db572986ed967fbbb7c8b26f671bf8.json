{"ast":null,"code":"import homeLayout from \"@/components/HomeView.vue\";\n// import store from \"../store\";\nimport axios from \"axios\";\n// import router from \"../router\";\n\nexport default {\n  components: {\n    homeLayout\n  },\n  data() {\n    return {\n      emp_account: \"\",\n      emp_password: \"\",\n      items: null,\n      empList: []\n    };\n  },\n  methods: {\n    queryItem() {\n      axios.post(\"/user/query\", {\n        emp_account: this.emp_account,\n        emp_password: this.emp_password\n      }).then(response => {\n        this.items = response.data;\n        console.log(this.items.ch_name);\n        this.empList = [];\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    queryAllItem() {\n      axios.post(\"/user/queryAll\").then(response => {\n        this.empList = response.data;\n        this.items = null;\n        console.log(this.empList[0]);\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["homeLayout","axios","components","data","emp_account","emp_password","items","empList","methods","queryItem","post","then","response","console","log","ch_name","catch","error","queryAllItem"],"sources":["C:\\Users\\eva97\\emp-repository\\demo\\emp-web\\src\\components\\TestView.vue"],"sourcesContent":["<template>\r\n  <!-- 套用HomeView.vue頁面 -->\r\n  <home-layout />\r\n  <div>\r\n    <div>\r\n      <input type=\"text\" v-model=\"emp_account\" placeholder=\"員工帳號\" />\r\n      <input type=\"text\" v-model=\"emp_password\" placeholder=\"員工密碼\" />\r\n      <button @click=\"queryItem\">單一查詢</button>\r\n    </div>\r\n    <div>\r\n      <button @click=\"queryAllItem\">查詢全部</button>\r\n    </div>\r\n    <div v-if=\"items\">\r\n      {{ items.ch_name }}\r\n    </div>\r\n    <ul v-if=\"empList.length > 0\">\r\n      <!-- <li v-for=\"(item, index) in empList\" :key=\"index\"> -->\r\n      <li v-for=\"item in empList\" :key=\"item.emp_no\">\r\n        {{ item.emp_account }} - {{ item.ch_name }}\r\n      </li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport homeLayout from \"@/components/HomeView.vue\";\r\n// import store from \"../store\";\r\nimport axios from \"axios\";\r\n// import router from \"../router\";\r\n\r\nexport default {\r\n  components: {\r\n    homeLayout,\r\n  },\r\n  data() {\r\n    return {\r\n      emp_account: \"\",\r\n      emp_password: \"\",\r\n      items: null,\r\n      empList: [],\r\n    };\r\n  },\r\n  methods: {\r\n    queryItem() {\r\n      axios\r\n        .post(\"/user/query\", {\r\n          emp_account: this.emp_account,\r\n          emp_password: this.emp_password,\r\n        })\r\n        .then((response) => {\r\n          this.items = response.data;\r\n          console.log(this.items.ch_name);\r\n          this.empList = [];\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    queryAllItem() {\r\n      axios\r\n        .post(\"/user/queryAll\")\r\n        .then((response) => {\r\n          this.empList  = response.data;\r\n          this.items= null;\r\n          console.log(this.empList[0]);\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.modal {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.modal-content {\r\n  background-color: #fff;\r\n  padding: 20px;\r\n  text-align: center;\r\n}\r\n\r\nbutton {\r\n  margin-top: 10px;\r\n}\r\n</style>\r\n"],"mappings":"AAyBA,OAAOA,UAAS,MAAO,2BAA2B;AAClD;AACA,OAAOC,KAAI,MAAO,OAAO;AACzB;;AAEA,eAAe;EACbC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,YAAY,EAAE,EAAE;MAChBC,KAAK,EAAE,IAAI;MACXC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAAA,EAAG;MACVR,KAAI,C