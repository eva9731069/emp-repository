n      $.ajax({\r\n        url: \"/user/add\",\r\n        method: \"POST\",\r\n        // eslint-disable-next-line no-unused-vars\r\n        success: function (response) {\r\n          alert(\"新增成功\");\r\n        },\r\n        error: function (error) {\r\n          console.error(error);\r\n        },\r\n      });\r\n    });\r\n    // eslint-disable-next-line no-undef\r\n    $(\"#deleteItem\").click(function () {\r\n      // eslint-disable-next-line no-undef\r\n      $.ajax({\r\n        url: \"/user/add\",\r\n        method: \"POST\",\r\n        // eslint-disable-next-line no-unused-vars\r\n        success: function (response) {\r\n          console.log(response.status);\r\n        },\r\n        error: function (error) {\r\n          console.error(error);\r\n        },\r\n      });\r\n    });\r\n  },\r\n  methods: {\r\n    queryItem() {\r\n      axios\r\n        .post(\"/user/query\", {\r\n          emp_account: this.emp_account,\r\n          emp_password: this.emp_password,\r\n        })\r\n        .then((response) => {\r\n          this.items = response.data;\r\n          console.log(this.items.ch_name);\r\n          this.empList = [];\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    queryAllItem() {\r\n      axios\r\n        .post(\"/user/queryAll\")\r\n        .then((response) => {\r\n          this.empList = response.data;\r\n          this.items = null;\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.modal {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.modal-content {\r\n  background-color: #fff;\r\n  padding: 20px;\r\n  text-align: center;\r\n}\r\n\r\nbutton {\r\n  margin-top: 10px;\r\n}\r\n</style>\r\n"],"mappings":"AAoCA,OAAO