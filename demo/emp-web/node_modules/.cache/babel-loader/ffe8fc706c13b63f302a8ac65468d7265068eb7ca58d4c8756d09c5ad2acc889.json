.then((response) => {\r\n          this.items = response.data;\r\n          console.log(this.items.ch_name);\r\n          this.empList = [];\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    queryAllItem() {\r\n      axios\r\n        .post(\"/user/queryAll\")\r\n        .then((response) => {\r\n          this.empList  = response.data;\r\n          this.items= null;\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.modal {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.modal-content {\r\n  background-color: #fff;\r\n  padding: 20px;\r\n  text-align: center;\r\n}\r\n\r\nbutton {\r\n  margin-top: 10px;\r\n}\r\n</style>\r\n"],"mappings":"AAyBA,OAAOA,UAAS,MAAO,2BAA2B;AAClD;AACA,OAAOC,KAAI,MAAO,OAAO;AACzB;;AAEA,eAAe;EACbC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,YAAY,EAAE,EAAE;MAChBC,KAAK,EAAE,IAAI;MACXC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EAACC,OAAOA,CAAA,EAAG;IACZ;IACA;IACAC,CAAC,CAAC,WAAW,CAAC,CAACC,KAAK,CAAC,YAAW;MAC9B;IAAA,CACD,CAAC;;IAEF;IACA;IACAD,CAAC,CAAC,YAAY,CAAC,CAACE,QAAQ,CAAC,WAAW,CAAC;;IAErC;IACA;IACAF,CAAC,CAACG,IAAI,CAAC;MACLC,GAAG,EAAE,aAAa;MAClBC,MAAM,EAAE,KAAK;MACb;MACAC,OAAO,EAAE,SAAAA,CAASC,QAAQ,EAAE;QAC1B;MAAA,CACD;MACD;MACAC,KAAK,EAAE,SAAAA,CAASA,KAAK,EAAE;QACrB;MAAA;IAEJ,CAAC,CAAC;EACJ,CAAC;EACCC,OAAO,EAAE;IACPC,SAASA,CAAA,EAAG;MACVlB,KAAI,CACDmB,IAAI,CAAC,aAAa,EAAE;QACnBhB,WAAW,EAAE,IAAI,CAACA,WAAW;QAC7BC,YAAY,EAAE,IAAI,CAACA;MACrB,CAAC,EACAgB,IAAI,CAAEL,QAAQ,IAAK;QAClB,IAAI,CAACV,KAAI,GAAIU,QAAQ,CAACb,IAAI;QAC1BmB,OAAO,CAACC,GAAG,CAAC,IAAI,CAACjB,KAAK,CAACkB,OAAO,CAAC;QAC/B,IAAI,CAACjB,OAAM,GAAI,EAAE;MACnB,CAAC,EACAkB,KAAK,CAAER,KAAK,IAAK;QAChBK,OAAO,CAACL,KAAK,CAACA,KAAK,CAAC;MAC