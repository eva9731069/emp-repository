{"ast":null,"code":"import homeLayout from \"@/components/HomeView.vue\";\n// import store from \"../store\";\nimport axios from \"axios\";\nimport \"bootstrap/dist/css/bootstrap.css\";\nimport \"bootstrap/dist/js/bootstrap.bundle\";\n// import router from \"../router\";\n\nexport default {\n  components: {\n    homeLayout\n  },\n  data() {\n    return {\n      emp_account: \"\",\n      emp_password: \"\",\n      items: null,\n      empList: [],\n      isShow: true\n    };\n  },\n  mounted() {\n    // eslint-disable-next-line no-undef\n    $(\"#addItem\").click(function () {\n      // eslint-disable-next-line no-undef\n      $.ajax({\n        url: \"/user/add\",\n        method: \"POST\",\n        data: {\n          // eslint-disable-next-line no-undef\n          emp_no: $(\"#emp_no\").val(),\n          // eslint-disable-next-line no-undef\n          emp_account: $(\"#emp_account\").val(),\n          // eslint-disable-next-line no-undef\n          emp_password: $(\"#emp_password\").val(),\n          // eslint-disable-next-line no-undef\n          ch_name: $(\"#ch_name\").val()\n        },\n        // eslint-disable-next-line no-unused-vars\n        success: function (response) {\n          alert(\"新增成功\");\n        },\n        error: function (error) {\n          console.error(error);\n        }\n      });\n    });\n    // eslint-disable-next-line no-undef\n    $(\"#addItem\").click(function () {\n      console.log('4455');\n      // eslint-disable-next-line no-undef\n      $.ajax({\n        url: \"/user/add\",\n        method: \"POST\",\n        data: {\n          // eslint-disable-next-line no-undef\n          emp_no: $(\"#emp_no\").val(),\n          // eslint-disable-next-line no-undef\n          emp_account: $(\"#emp_account\").val(),\n          // eslint-disable-next-line no-undef\n          emp_password: $(\"#emp_password\").val(),\n          // eslint-disable-next-line no-undef\n          ch_name: $(\"#ch_name\").val()\n        },\n        // eslint-disable-next-line no-unused-vars\n        success: function (response) {\n          console.log(response.status);\n        },\n        error: function (error) {\n          console.error(error);\n        }\n      });\n    });\n  },\n  methods: {\n    queryItem() {\n      axios.post(\"/user/query\", {\n        emp_account: this.emp_account,\n        emp_password: this.emp_password\n      }).then(response => {\n        this.empList = response.data;\n        console.log(\"length\" + this.empList.length);\n        //res=>vo用這個寫法\n        // this.items = response.data;\n        // console.log(this.items.ch_name);\n        // this.empList = [];\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    queryAllItem() {\n      axios.post(\"/user/queryAll\").then(response => {\n        this.empList = response.data;\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    close() {\n      this.isShow = !this.isShow;\n    },\n    open() {\n      this.isShow = !this.isShow;\n    }\n  }\n};","map":{"version":3,"names":["homeLayout","axios","components","data","emp_account","emp_password","items","empList","isShow","mounted","$","click","ajax","url","method","emp_no","val","ch_name","success","response","alert","error","console","log","status","methods","queryItem","post","then","length","catch","queryAllItem","close","open"],"sources":["C:\\Users\\eva97\\emp-repository\\demo\\emp-web\\src\\components\\TestView.vue"],"sourcesContent":["<template>\r\n  <!-- 套用HomeView.vue頁面 -->\r\n  <home-layout />\r\n  <div class=\"col-9\">\r\n    <div>\r\n      <input type=\"text\" v-model=\"emp_account\" placeholder=\"員工帳號\" />\r\n      <input type=\"text\" v-model=\"emp_password\" placeholder=\"員工密碼\" />\r\n      <button @click=\"queryItem\">單一查詢</button>\r\n      <button @click=\"queryAllItem\">查詢全部</button>\r\n      <button @click=\"open\">新增</button>\r\n\r\n      <div class=\"from modal-body\" v-show=\"!isShow\">\r\n        <div class=\"modal\" id=\"myModal\">\r\n          <div class=\"modal-dialog\">\r\n            <div class=\"modal-content\">\r\n              <div class=\"modal-body\">\r\n                <input\r\n                  type=\"text\"\r\n                  id=\"emp_no\"\r\n                  placeholder=\"員工編號\"\r\n                /><br /><br />\r\n                <input\r\n                  type=\"text\"\r\n                  id=\"emp_account\"\r\n                  placeholder=\"員工帳號\"\r\n                /><br /><br />\r\n                <input\r\n                  type=\"text\"\r\n                  id=\"emp_password\"\r\n                  placeholder=\"員工密碼\"\r\n                /><br /><br />\r\n                <input\r\n                  type=\"text\"\r\n                  id=\"ch_name\"\r\n                  placeholder=\"員工姓名\"\r\n                /><br /><br />\r\n              </div>\r\n              <div class=\"modal-footer\">\r\n                <button\r\n                  id=\"addItem\"\r\n                  @click=\"close\"\r\n                  type=\"button\"\r\n                  class=\"btn btn-danger\"\r\n                  data-bs-dismiss=\"modal\"\r\n                >\r\n                  新增\r\n                </button>\r\n                <button\r\n                  @click=\"close\"\r\n                  type=\"button\"\r\n                  class=\"btn btn-danger\"\r\n                  data-bs-dismiss=\"modal\"\r\n                >\r\n                  Close\r\n                </button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <div>\r\n    <br />\r\n    <br />\r\n    <br />\r\n  </div>\r\n  <div class=\"col-12\">\r\n    <table class=\"empTable table-sm table-bordered\" v-if=\"empList.length > 0\">\r\n      <thead>\r\n        <tr>\r\n          <th>員工編號</th>\r\n          <th>員工帳號</th>\r\n          <th>員工密碼</th>\r\n          <th>員工姓名</th>\r\n          <th>修改</th>\r\n          <th>刪除</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"item in empList\" :key=\"item.emp_no\">\r\n          <td>{{ item.emp_no }}</td>\r\n          <td>{{ item.emp_account }}</td>\r\n          <td>{{ item.emp_password }}</td>\r\n          <td>{{ item.ch_name }}</td>\r\n          <td><button id=\"editItem\">修改</button></td>\r\n          <td><button id=\"deleteItem\">刪除</button></td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport homeLayout from \"@/components/HomeView.vue\";\r\n// import store from \"../store\";\r\nimport axios from \"axios\";\r\nimport \"bootstrap/dist/css/bootstrap.css\";\r\nimport \"bootstrap/dist/js/bootstrap.bundle\";\r\n// import router from \"../router\";\r\n\r\nexport default {\r\n  components: {\r\n    homeLayout,\r\n  },\r\n  data() {\r\n    return {\r\n      emp_account: \"\",\r\n      emp_password: \"\",\r\n      items: null,\r\n      empList: [],\r\n      isShow: true,\r\n    };\r\n  },\r\n  mounted() {\r\n    // eslint-disable-next-line no-undef\r\n    $(\"#addItem\").click(function () {\r\n      // eslint-disable-next-line no-undef\r\n      $.ajax({\r\n        url: \"/user/add\",\r\n        method: \"POST\",\r\n        data: {\r\n          // eslint-disable-next-line no-undef\r\n          emp_no: $(\"#emp_no\").val(),\r\n          // eslint-disable-next-line no-undef\r\n          emp_account: $(\"#emp_account\").val(),\r\n          // eslint-disable-next-line no-undef\r\n          emp_password: $(\"#emp_password\").val(),\r\n          // eslint-disable-next-line no-undef\r\n          ch_name: $(\"#ch_name\").val(),\r\n        },\r\n        // eslint-disable-next-line no-unused-vars\r\n        success: function (response) {\r\n          alert(\"新增成功\");\r\n        },\r\n        error: function (error) {\r\n          console.error(error);\r\n        },\r\n      });\r\n    });\r\n    // eslint-disable-next-line no-undef\r\n    $(\"#addItem\").click(function () {\r\n      console.log('4455');\r\n      // eslint-disable-next-line no-undef\r\n      $.ajax({\r\n        url: \"/user/add\",\r\n        method: \"POST\",\r\n        data: {\r\n          // eslint-disable-next-line no-undef\r\n          emp_no: $(\"#emp_no\").val(),\r\n          // eslint-disable-next-line no-undef\r\n          emp_account: $(\"#emp_account\").val(),\r\n          // eslint-disable-next-line no-undef\r\n          emp_password: $(\"#emp_password\").val(),\r\n          // eslint-disable-next-line no-undef\r\n          ch_name: $(\"#ch_name\").val(),\r\n        },\r\n        // eslint-disable-next-line no-unused-vars\r\n        success: function (response) {\r\n          console.log(response.status);\r\n        },\r\n        error: function (error) {\r\n          console.error(error);\r\n        },\r\n      });\r\n    });\r\n  },\r\n  methods: {\r\n    queryItem() {\r\n      axios\r\n        .post(\"/user/query\", {\r\n          emp_account: this.emp_account,\r\n          emp_password: this.emp_password,\r\n        })\r\n        .then((response) => {\r\n          this.empList = response.data;\r\n          console.log(\"length\" + this.empList.length);\r\n          //res=>vo用這個寫法\r\n          // this.items = response.data;\r\n          // console.log(this.items.ch_name);\r\n          // this.empList = [];\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    queryAllItem() {\r\n      axios\r\n        .post(\"/user/queryAll\")\r\n        .then((response) => {\r\n          this.empList = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    close() {\r\n      this.isShow = !this.isShow;\r\n    },\r\n    open() {\r\n      this.isShow = !this.isShow;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.empTable {\r\n  margin-left: 600px;\r\n}\r\n.modal {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  