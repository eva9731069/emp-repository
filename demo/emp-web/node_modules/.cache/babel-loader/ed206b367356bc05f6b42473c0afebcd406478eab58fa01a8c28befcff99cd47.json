{"ast":null,"code":"import homeLayout from \"@/components/HomeView.vue\";\n\n// import store from \"../store\";\nimport axios from \"axios\";\nimport \"bootstrap/dist/css/bootstrap.css\";\nimport \"bootstrap/dist/js/bootstrap.bundle\";\n\n// import PropsChild from '@/components/EditView.vue'\n// import TodoEdit from '@/components/EditView.vue';\n// import editLayout from '@/components/HomeView.vue';\n// import router from \"../router\";\n\nexport default {\n  components: {\n    homeLayout,\n    // eslint-disable-next-line no-undef\n    AddPage\n    // PropsChild\n  },\n\n  data() {\n    return {\n      emp_account: \"\",\n      emp_password: \"\",\n      items: null,\n      empList: [],\n      isShow: true,\n      isUpdate: false,\n      isAddPageVisible: false\n      // contentData:'我是父组件的数据********'\n    };\n  },\n\n  mounted() {\n    // eslint-disable-next-line no-undef\n    $(\"#addItem\").click(function () {\n      // eslint-disable-next-line no-undef\n      $.ajax({\n        url: \"/user/add\",\n        method: \"POST\",\n        data: {\n          // eslint-disable-next-line no-undef\n          emp_no: $(\"#emp_no\").val(),\n          // eslint-disable-next-line no-undef\n          emp_account: $(\"#emp_account\").val(),\n          // eslint-disable-next-line no-undef\n          emp_password: $(\"#emp_password\").val(),\n          // eslint-disable-next-line no-undef\n          ch_name: $(\"#ch_name\").val()\n        },\n        // eslint-disable-next-line no-unused-vars\n        success: function (response) {\n          alert(\"新增成功\");\n        },\n        error: function (error) {\n          console.error(error);\n        }\n      });\n    });\n  },\n  methods: {\n    queryItem() {\n      axios.post(\"/user/query\", {\n        emp_account: this.emp_account,\n        emp_password: this.emp_password\n      }).then(response => {\n        this.empList = response.data;\n        console.log(\"length\" + this.empList.length);\n        //res=>vo用這個寫法\n        // this.items = response.data;\n        // console.log(this.items.ch_name);\n        // this.empList = [];\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    queryAllItem() {\n      axios.post(\"/user/queryAll\").then(response => {\n        this.empList = response.data;\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    deleteItem(emp_no) {\n      console.log(emp_no);\n      axios.post(\"/user/delete\", {\n        emp_no: emp_no\n      }).then(response => {\n        alert(\"刪除成功\");\n        console.log(response.status);\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    close() {\n      this.isShow = !this.isShow;\n    },\n    add() {\n      this.isShow = !this.isShow;\n    },\n    showAddPage() {\n      this.isAddPageVisible = true;\n    },\n    closeAddPage() {\n      this.isAddPageVisible = false;\n    },\n    editItem(emp_no, emp_account, emp_password, ch_name) {\n      this.isShow = !this.isShow;\n      console.log(emp_no);\n      axios.post(\"/user/edit\"), {\n        emp_no: emp_no,\n        emp_account: emp_account,\n        emp_password: emp_password,\n        ch_name: ch_name\n      }.then(response => {\n        this.empList = response.data;\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["homeLayout","axios","components","AddPage","data","emp_account","emp_password","items","empList","isShow","isUpdate","isAddPageVisible","mounted","$","click","ajax","url","method","emp_no","val","ch_name","success","response","alert","error","console","methods","queryItem","post","then","log","length","catch","queryAllItem","deleteItem","status","close","add","showAddPage","closeAddPage","editItem"],"sources":["C:\\Users\\eva97\\emp-repository\\demo\\emp-web\\src\\components\\TestView.vue"],"sourcesContent":["<template>\r\n  <!-- 套用HomeView.vue頁面 -->\r\n  <home-layout />\r\n  <div class=\"col-9\">\r\n    <div>\r\n      <input type=\"text\" v-model=\"emp_account\" placeholder=\"員工帳號\" />\r\n      <input type=\"text\" v-model=\"emp_password\" placeholder=\"員工密碼\" />\r\n      <button @click=\"queryItem\">單一查詢</button>\r\n      <button @click=\"queryAllItem\">查詢全部</button>\r\n      <!-- <button @click=\"open\">Account</button>\r\n      <add-layout /> -->\r\n      <!-- <button @click=\"add\">新增</button> -->\r\n      <button @click=\"showAddPage\">新增</button>\r\n      <div v-if=\"isAddPageVisible\" class=\"modal\">\r\n      <div class=\"modal-content\">\r\n        <span class=\"close\" @click=\"closeAddPage\">&times;</span>\r\n        <add-page></add-page>\r\n      </div>\r\n    </div>\r\n      \r\n    </div>\r\n  </div>\r\n  <div>\r\n    <br />\r\n    <br />\r\n    <br />\r\n  </div>\r\n  <div class=\"col-12\">\r\n    <table class=\"empTable table-sm table-bordered\" v-if=\"empList.length > 0\">\r\n      <thead>\r\n        <tr>\r\n          <th>員工編號</th>\r\n          <th>員工帳號</th>\r\n          <th>員工密碼</th>\r\n          <th>員工姓名</th>\r\n          <th>修改</th>\r\n          <th>刪除</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"item in empList\" :key=\"item.emp_no\">\r\n          <td>{{ item.emp_no }}</td>\r\n          <td>{{ item.emp_account }}</td>\r\n          <td>{{ item.emp_password }}</td>\r\n          <td>{{ item.ch_name }}</td>\r\n          <td>\r\n            <button @click=\"editItem(\r\n                  item.emp_no,\r\n                  item.emp_account,\r\n                  item.emp_password,\r\n                  item.ch_name)\">修改\r\n            </button>\r\n          </td>\r\n          <td><button @click=\"deleteItem(item.emp_no)\">刪除</button></td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport homeLayout from \"@/components/HomeView.vue\";\r\n\r\n// import store from \"../store\";\r\nimport axios from \"axios\";\r\nimport \"bootstrap/dist/css/bootstrap.css\";\r\nimport \"bootstrap/dist/js/bootstrap.bundle\";\r\n\r\n// import PropsChild from '@/components/EditView.vue'\r\n// import TodoEdit from '@/components/EditView.vue';\r\n// import editLayout from '@/components/HomeView.vue';\r\n// import router from \"../router\";\r\n\r\nexport default {\r\n  components: {\r\n    homeLayout,\r\n    // eslint-disable-next-line no-undef\r\n    AddPage\r\n    // PropsChild\r\n  },\r\n  data() {\r\n    return {\r\n      emp_account: \"\",\r\n      emp_password: \"\",\r\n      items: null,\r\n      empList: [],\r\n      isShow: true,\r\n      isUpdate: false,\r\n      isAddPageVisible: false\r\n      // contentData:'我是父组件的数据********'\r\n    };\r\n  },\r\n  mounted() {\r\n    // eslint-disable-next-line no-undef\r\n    $(\"#addItem\").click(function () {\r\n      // eslint-disable-next-line no-undef\r\n      $.ajax({\r\n        url: \"/user/add\",\r\n        method: \"POST\",\r\n        data: {\r\n          // eslint-disable-next-line no-undef\r\n          emp_no: $(\"#emp_no\").val(),\r\n          // eslint-disable-next-line no-undef\r\n          emp_account: $(\"#emp_account\").val(),\r\n          // eslint-disable-next-line no-undef\r\n          emp_password: $(\"#emp_password\").val(),\r\n          // eslint-disable-next-line no-undef\r\n          ch_name: $(\"#ch_name\").val(),\r\n        },\r\n        // eslint-disable-next-line no-unused-vars\r\n        success: function (response) {\r\n          alert(\"新增成功\");\r\n        },\r\n        error: function (error) {\r\n          console.error(error);\r\n        },\r\n      });\r\n    });\r\n  },\r\n  methods: {\r\n    queryItem() {\r\n      axios\r\n        .post(\"/user/query\", {\r\n          emp_account: this.emp_account,\r\n          emp_password: this.emp_password,\r\n        })\r\n        .then((response) => {\r\n          this.empList = response.data;\r\n          console.log(\"length\" + this.empList.length);\r\n          //res=>vo用這個寫法\r\n          // this.items = response.data;\r\n          // console.log(this.items.ch_name);\r\n          // this.empList = [];\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    queryAllItem() {\r\n      axios\r\n        .post(\"/user/queryAll\")\r\n        .then((response) => {\r\n          this.empList = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    deleteItem(emp_no) {\r\n      console.log(emp_no);\r\n      axios\r\n        .post(\"/user/delete\", {\r\n          emp_no: emp_no,\r\n        })\r\n        .then((response) => {\r\n          alert(\"刪除成功\");\r\n          console.log(response.status);\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n\r\n    close() {\r\n      this.isShow = !this.isShow;\r\n    },\r\n    add() {\r\n      this.isShow = !this.isShow;\r\n    },\r\n   showAddPage() {\r\n      this.isAddPageVisible = true;\r\n    },\r\n    closeAddPage() {\r\n      this.isAddPageVisible = false;\r\n    },\r\n    editItem(emp_no, emp_account, emp_password, ch_name) {\r\n      this.isShow = !this.isShow;\r\n      console.log(emp_no);\r\n      axios.post(\"/user/edit\"),\r\n        {\r\n          emp_no: emp_no,\r\n          emp_account: emp_account,\r\n          emp_password: emp_password,\r\n          ch_name: ch_name,\r\n        }\r\n          .then((response) => {\r\n            this.empList = response.data;\r\n          })\r\n          .catch((error) => {\r\n            console.error(error);\r\n          });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.empTable {\r\n  margin-left: 600px;\r\n}\r\n.modal {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.modal h2 {\r\n  color: #fff;\r\n}\r\n\r\n.modal button {\r\n  margin-top: 10px;\r\n}\r\n\r\nbutton {\r\n  margin-top: 10px;\r\n}\r\n\r\n.testca {\r\n  justify-content: center;\r\n}\r\n</style>\r\n"],"mappings":"AA6DA,OAAOA,UAAS,MAAO,2BAA2B;;AAElD;AACA,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAO,kCAAkC;AACzC,OAAO,oCAAoC;;AAE3C;AACA;AACA;AACA;;AAEA,eAAe;EACbC,UAAU,EAAE;IACVF,UAAU;IACV;IACAG;IACA;EACF,CAAC;;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,YAAY,EAAE,EAAE;MAChBC,KAAK,EAAE,IAAI;MACXC,OAAO,EAAE,EAAE;MACXC,MAAM,EAAE,IAAI;MACZC,QAAQ,EAAE,KAAK;MACfC,gBAAgB,EAAE;MAClB;IACF,CAAC;EACH,CAAC;;EACDC,OAAOA,CAAA,EAAG;IACR;IACAC,CAAC,CAAC,UAAU,CAAC,CAACC,KAAK,CAAC,YAAY;MAC9B;MACAD,CAAC,CAACE,IAAI,CAAC;QACLC,GAAG,EAAE,WAAW;QAChBC,MAAM,EAAE,MAAM;QACdb,IAAI,EAAE;UACJ;UACAc,MAAM,EAAEL,CAAC,CAAC,SAAS,CAAC,CAACM,GAAG,CAAC,CAAC;UAC1B;UACAd,WAAW,EAAEQ,CAAC,CAAC,cAAc,CAAC,CAACM,GAAG,CAAC,CAAC;UACpC;UACAb,YAAY,EAAEO,CAAC,CAAC,eAAe,CAAC,CAACM,GAAG,CAAC,CAAC;UACtC;UACAC,OAAO,EAAEP,CAAC,CAAC,UAAU,CAAC,CAACM,GAAG,CAAC;QAC7B,CAAC;QACD;QACAE,OAAO,EAAE,SAAAA,CAAUC,QAAQ,EAAE;UAC3BC,KAAK,CAAC,MAAM,CAAC;QACf,CAAC;QACDC,KAAK,EAAE,SAAAA,CAAUA,KAAK,EAAE;UACtBC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;QACtB;MACF,CAAC,CAAC;IACJ,CAAC,CAAC;EACJ,CAAC;EACDE,OAAO,EAAE;IACPC,SAASA,CAAA,EAAG;MACV1B,KAAI,CACD2B,IAAI,CAAC,aAAa,EAAE;QACnBvB,WAAW,EAAE,IAAI,CAACA,WAAW;QAC7BC,YAAY,EAAE,IAAI,CAACA;MACrB,CAAC,EACAuB,IAAI,CAAEP,QAAQ,IAAK;QAClB,IAAI,CAACd,OAAM,GAAIc,QAAQ,CAAClB,IAAI;QAC5BqB,OAAO,CAACK,GAAG,CAAC,QAAO,GAAI,IAAI,CAACtB,OAAO,CAACuB,MAAM,CAAC;QAC3C;QACA;QACA;QACA;MACF,CAAC,EACAC,KAAK,CAAER,KAAK,IAAK;QAChBC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACN,CAAC;IACDS,YAAYA,CAAA,EAAG;MACbhC,KAAI,CACD2B,IAAI,CAAC,gBAAgB,EACrBC,IAAI,CAAEP,QAAQ,IAAK;QAClB,IAAI,CAACd,OAAM,GAAIc,QAAQ,CAAClB,IAAI;MAC9B,CAAC,EACA4B,KAAK,CAAER,KAAK,IAAK;QAChBC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACN,CAAC;IACDU,UAAUA,CAAChB,MAAM,EAAE;MACjBO,OAAO,CAACK,GAAG,CAACZ,MAAM,CAAC;MACnBjB,KAAI,CACD2B,IAAI,CAAC,cAAc,EAAE;QACpBV,MAAM,EAAEA;MACV,CAAC,EACAW,IAAI,CAAEP,QAAQ,IAAK;QAClBC,KAAK,CAAC,MAAM,CAAC;QACbE,OAAO,CAACK,GA