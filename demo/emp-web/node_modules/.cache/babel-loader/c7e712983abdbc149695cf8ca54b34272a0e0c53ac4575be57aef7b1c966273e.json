{"ast":null,"code":"import homeLayout from \"@/components/HomeView.vue\";\n// import store from \"../store\";\nimport axios from \"axios\";\nimport \"bootstrap/dist/css/bootstrap.css\";\nimport \"bootstrap/dist/js/bootstrap.bundle\";\n// import PropsChild from '@/components/EditView.vue'\n// import TodoEdit from '@/components/EditView.vue';\n// import editLayout from '@/components/HomeView.vue';\n// import router from \"../router\";\n\nexport default {\n  components: {\n    homeLayout\n    // PropsChild\n  },\n\n  data() {\n    return {\n      emp_account: \"\",\n      emp_password: \"\",\n      items: null,\n      empList: [],\n      isShow: true,\n      isUpdate: false\n      // contentData:'我是父组件的数据********'\n    };\n  },\n\n  mounted() {\n    // eslint-disable-next-line no-undef\n    $(\"#addItem\").click(function () {\n      // eslint-disable-next-line no-undef\n      $.ajax({\n        url: \"/user/add\",\n        method: \"POST\",\n        data: {\n          // eslint-disable-next-line no-undef\n          emp_no: $(\"#emp_no\").val(),\n          // eslint-disable-next-line no-undef\n          emp_account: $(\"#emp_account\").val(),\n          // eslint-disable-next-line no-undef\n          emp_password: $(\"#emp_password\").val(),\n          // eslint-disable-next-line no-undef\n          ch_name: $(\"#ch_name\").val()\n        },\n        // eslint-disable-next-line no-unused-vars\n        success: function (response) {\n          alert(\"新增成功\");\n        },\n        error: function (error) {\n          console.error(error);\n        }\n      });\n    });\n    // eslint-disable-next-line no-undef\n    $(\"#deleteItem\").click(function () {\n      // eslint-disable-next-line no-undef\n      $.ajax({\n        url: \"/user/delete\",\n        method: \"POST\",\n        data: {\n          // eslint-disable-next-line no-undef\n          emp_no: $(\"#emp_no\").val(),\n          // eslint-disable-next-line no-undef\n          emp_account: $(\"#emp_account\").val(),\n          // eslint-disable-next-line no-undef\n          emp_password: $(\"#emp_password\").val(),\n          // eslint-disable-next-line no-undef\n          ch_name: $(\"#ch_name\").val()\n        },\n        // eslint-disable-next-line no-unused-vars\n        success: function (response) {\n          alert(\"新增成功\");\n        },\n        error: function (error) {\n          console.error(error);\n        }\n      });\n    });\n  },\n  methods: {\n    queryItem() {\n      axios.post(\"/user/query\", {\n        emp_account: this.emp_account,\n        emp_password: this.emp_password\n      }).then(response => {\n        this.empList = response.data;\n        console.log(\"length\" + this.empList.length);\n        //res=>vo用這個寫法\n        // this.items = response.data;\n        // console.log(this.items.ch_name);\n        // this.empList = [];\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    queryAllItem() {\n      axios.post(\"/user/queryAll\").then(response => {\n        this.empList = response.data;\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    deleteItem() {\n      axios.post(\"/user/delete\", {\n        emp_account: this.emp_account,\n        emp_password: this.emp_password\n      }).then(response => {\n        alert(response.status);\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    close() {\n      this.isShow = !this.isShow;\n    },\n    add() {\n      this.isShow = !this.isShow;\n    },\n    editItem(emp_no, emp_account, emp_password, ch_name) {\n      this.isShow = !this.isShow;\n      console.log(emp_no);\n      axios.post(\"/user/edit\"), {\n        emp_no: emp_no,\n        emp_account: emp_account,\n        emp_password: emp_password,\n        ch_name: ch_name\n      }.then(response => {\n        this.empList = response.data;\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["homeLayout","axi