"addItem\"\r\n                  @click=\"close\"\r\n                  type=\"button\"\r\n                  class=\"btn btn-danger\"\r\n                  data-bs-dismiss=\"modal\"\r\n                >\r\n                  確認\r\n                </button>\r\n                <button\r\n                  @click=\"close\"\r\n                  type=\"button\"\r\n                  class=\"btn btn-danger\"\r\n                  data-bs-dismiss=\"modal\"\r\n                >\r\n                  Close\r\n                </button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <div>\r\n    <br />\r\n    <br />\r\n    <br />\r\n  </div>\r\n  <div class=\"col-12\">\r\n    <table class=\"empTable table-sm table-bordered\" v-if=\"empList.length > 0\">\r\n      <thead>\r\n        <tr>\r\n          <th>員工編號</th>\r\n          <th>員工帳號</th>\r\n          <th>員工密碼</th>\r\n          <th>員工姓名</th>\r\n          <th>修改</th>\r\n          <th>刪除</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"item in empList\" :key=\"item.emp_no\">\r\n          <td>{{ item.emp_no }}</td>\r\n          <td>{{ item.emp_account }}</td>\r\n          <td>{{ item.emp_password }}</td>\r\n          <td>{{ item.ch_name }}</td>\r\n          <!-- <td><button id=\"editItem\">修改</button></td> -->\r\n          <td><button @click=\"editItem(item.emp_no,item.emp_account,item.emp_password,item.ch_name)\">修改</button></td>\r\n          <!-- <PropsChild v-model=\"contentData\"></PropsChild> -->\r\n          <!-- <td><button @click=\"editItem(item)\">修改</button></td> -->\r\n          <!-- <td><button @click=\"editItem(item)\">修改</button></td> -->\r\n          <!-- <router-link :to='{name:\"categoryEdit\",params:{emp_no:item.emp_no}}' class=\"btn btn-success\">Edit</router-link> -->\r\n<!-- <edit-layout /> -->\r\n\r\n    \r\n          <td><button @click=\"deleteItem(item.emp_no)\">刪除</button></td>\r\n          <!-- <td><button id=\"addItem\">刪除</button></td> -->\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport homeLayout from \"@/components/HomeView.vue\";\r\n// import store from \"../store\";\r\nimport axios from \"axios\";\r\nimport \"bootstrap/dist/css/bootstrap.css\";\r\nimport \"bootstrap/dist/js/bootstrap.bundle\";\r\n// import PropsChild from '@/components/EditView.vue'\r\n// import TodoEdit from '@/components/EditView.vue';\r\n// import editLayout from '@/components/HomeView.vue';\r\n// import router from \"../router\";\r\n\r\nexport default {\r\n  components: {\r\n    homeLayout,\r\n    // PropsChild\r\n  },\r\n  data() {\r\n    return {\r\n      emp_account: \"\",\r\n      emp_password: \"\",\r\n      items: null,\r\n      empList: [],\r\n      isShow: true,\r\n      isUpdate:false,\r\n      // contentData:'我是父组件的数据********'\r\n    };\r\n  },\r\n  mounted() {\r\n    // eslint-disable-next-line no-undef\r\n    $(\"#addItem\").click(function () {\r\n      // eslint-disable-next-line no-undef\r\n      $.ajax({\r\n        url: \"/user/add\",\r\n        method: \"POST\",\r\n        data: {\r\n          // eslint-disable-next-line no-undef\r\n          emp_no: $(\"#emp_no\").val(),\r\n          // eslint-disable-next-line no-undef\r\n          emp_account: $(\"#emp_account\").val(),\r\n          // eslint-disable-next-line no-undef\r\n          emp_password: $(\"#emp_password\").val(),\r\n          // eslint-disable-next-line no-undef\r\n          ch_name: $(\"#ch_name\").val(),\r\n        },\r\n        // eslint-disable-next-line no-unused-vars\r\n        success: function (response) {\r\n          alert(\"新增成功\");\r\n        },\r\n        error: function (error) {\r\n          console.error(error);\r\n        },\r\n      });\r\n    });\r\n    // eslint-disable-next-line no-undef\r\n    $(\"#deleteItem\").click(function () {\r\n      // eslint-disable-next-line no-undef\r\n      $.ajax({\r\n        url: \"/user/delete\",\r\n        method: \"POST\",\r\n        data: {\r\n          // eslint-disable-next-line no-undef\r\n          emp_no: $(\"#emp_no\").val(),\r\n          // eslint-disable-next-line no-undef\r\n          emp_account: $(\"#emp_account\").val(),\r\n          // eslint-disable-next-line no-undef\r\n          emp_password: $(\"#emp_password\").val(),\r\n          // eslint-disable-next-line no-undef\r\n          ch_name: $(\"#ch_name\").val(),\r\n        },\r\n        // eslint-disable-next-line no-unused-vars\r\n        success: function (response) {\r\n          alert(\"新增成功\");\r\n        },\r\n        error: function (error) {\r\n          console.error(error);\r\n        },\r\n      });\r\n    });\r\n  },\r\n  methods: {\r\n    queryItem() {\r\n      axios\r\n        .post(\"/user/query\", {\r\n          emp_account: this.emp_account,\r\n          emp_password: this.emp_password,\r\n        })\r\n        .then((response) => {\r\n          this.empList = response.data;\r\n          console.log(\"length\" + this.empList.length);\r\n          //res=>vo用這個寫法\r\n          // this.items = response.data;\r\n          // console.log(this.items.ch_name);\r\n          // this.empList = [];\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    queryAllItem() {\r\n      axios\r\n        .post(\"/user/queryAll\")\r\n        .then((response) => {\r\n          this.empList = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    deleteItem() {\r\n      axios\r\n        .post(\"/user/delete\", {\r\n          emp_account: this.emp_account,\r\n          emp_password: this.emp_password,\r\n        })\r\n        .then((response) => {\r\n          alert(response.status);\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    \r\n    close() {\r\n      this.isShow = !this.isShow;\r\n    },\r\n    add() {\r\n      this.isShow = !this.isShow;\r\n    },\r\n    \r\n    editItem(emp_no, emp_account, emp_password, ch_name) {\r\n      this.isShow = !this.isShow;\r\n      console.log(emp_no);\r\n      axios\r\n        .post(\"/user/edit\"),{\r\n          emp_no: emp_no,\r\n          emp_account: emp_account,\r\n          emp_password: emp_password,\r\n          ch_name: ch_name,\r\n        }\r\n        .then((response) => {\r\n          this.empList = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.empTable {\r\n  margin-left: 600px;\r\n}\r\n.modal {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.modal h2 {\r\n  color: #fff;\r\n}\r\n\r\n.modal button {\r\n  margin-top: 10px;\r\n}\r\n\r\nbutton {\r\n  margin-top: 10px;\r\n}\r\n\r\n.testca {\r\n  justify-content: center;\r\n}\r\n</style>\r\n"],"mappings":"AAyGA,OAAOA,UAAS,MAAO,2BAA2B;AAClD;AACA,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAO,kCAAkC;AACzC,OAAO,oCAAoC;AAC3C;AACA;AACA;AACA;;AAEA,eAAe;EACbC,UAAU,EAAE;IACVF;IACA;EACF,CAAC;;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,YAAY,EAAE,EAAE;MAChBC,KAAK,EAAE,IAAI;MACXC,OAAO,EAAE,EAAE;MACXC,MAAM,EAAE,IAAI;MACZC,QAAQ,EAAC;MACT;IACF,CAAC;EACH,CAAC;;EACDC,OAAOA,CAAA,EAAG;IACR;IACAC,CAAC,CAAC,UAAU,CAAC,CAACC,KAAK,CAAC,YAAY;MAC9B;MACAD,CAAC,CAACE,IAAI,CAAC;QACLC,GAAG,EAAE,WAAW;QAChBC,MAAM,EAAE,MAAM;QACdZ,IAAI,EAAE;UACJ;UACAa,MAAM,EAAEL,CAAC,CAAC,SAAS,CAAC,CAACM,GAAG,CAAC,CAAC;UAC1B;UACAb,WAAW,EAAEO,CAAC,CAAC,cAAc,CAAC,CAACM,GAAG,CAAC,CAAC;UACpC;UACAZ,YAAY,EAAEM,CAAC,CAAC,eAAe,CAAC,CAACM,GAAG,CAAC,CAAC;UACtC;UACAC,OAAO,EAAEP,CAAC,CAAC,UAAU,CAAC,CAACM,GAAG,CAAC;QAC7B,CAAC;QACD;QACAE,OAAO,EAAE,SAAAA,CAAUC,QAAQ,EAAE;UAC3BC,KAAK,CAAC,MAAM,CAAC;QACf,CAAC;QACDC,KAAK,EAAE,SAAAA,CAAUA,KAAK,EAAE;UACtBC,OA